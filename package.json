{
  "name": "code-test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"babel-node ./bin/www --presets es2015\" ",
    "start-dev": "tsc && concurrently \"npm run tsc:w\" \"babel-node.js ./bin/www --presets es2015\" ",
    "test": "DB=localTest ./node_modules/.bin/mocha --compilers js:babel-core/register --timeout 10000",
    "coverage": "DB=localTest ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --timeout 10000 --compilers js:babel-core/register",
    "postinstall": "npm install typescript -g && npm install concurrently -g && npm install babel-cli -g && npm install babel-preset-es2015",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "dependencies": {
    "@angular/common": "~2.0.1",
    "@angular/compiler": "~2.0.1",
    "@angular/core": "~2.0.1",
    "@angular/forms": "~2.0.1",
    "@angular/http": "~2.0.1",
    "@angular/platform-browser": "~2.0.1",
    "@angular/platform-browser-dynamic": "~2.0.1",
    "@angular/router": "~3.0.1",
    "@angular/upgrade": "~2.0.1",
    "angular-in-memory-web-api": "~0.1.1",
    "angular2-moment": "^1.0.0-beta.5",
    "body-parser": "~1.15.1",
    "bootstrap": "^3.3.7",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "moment": "^2.15.1",
    "mongoose": "^4.6.3",
    "morgan": "~1.7.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "serve-favicon": "~2.3.0",
    "systemjs": "0.19.39",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "concurrently": "^3.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "lite-server": "^2.2.2",
    "mocha": "^3.1.2",
    "supertest": "^2.0.0",
    "typescript": "^2.0.3",
    "typings": "^1.4.0"
  }
